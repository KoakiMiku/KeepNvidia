      "companyName": "Tunnel Vision Laboratories, LLC",
      "copyrightText": "Copyright (c) {companyName}. All rights reserved.\nLicensed under the {licenseName} license. See LICENSE file in the project root for full license information.",
      "variables": {
        "licenseName": "MIT"
      }
    },
    "layoutRules": {
      "newlineAtEndOfFile": "require"
    }
  }
}
